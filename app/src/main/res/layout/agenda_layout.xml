<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/agenda.celcatButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="celcat"
        android:text="@string/celcat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/agenda.importButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="importICS"
        android:text="@string/import_ics"
        app:layout_constraintBottom_toTopOf="@+id/agenda.celcatButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>